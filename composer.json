        "drupal/thunder_admin": "^3.3",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.1",
        "drupal/update_helper": "^1.3",
        "drupal/video_embed_facebook": "^1.3",
        "drupal/video_embed_field": "^2.0",
        "drupal/view_mode_selector": "1.x@dev",
        "drupal/views_data_export": "^1.0@beta",
        "drupal/views_field_formatter": "^1.0",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0",
        "cweagans/composer-patches": "^1.6",
        "drupal/stage_file_proxy": "^1.0@beta"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "remove-git-submodules": "find . -mindepth 2 -type d -name .git | xargs rm -rf",
        "undo-composer-changes-to-dev-services-file": "git checkout sites/development.services.yml",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@remove-git-submodules"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "@remove-git-submodules",
            "@undo-composer-changes-to-dev-services-file"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"],
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "enable-patching": "true",
        "patches": {
            "drupal/commerce_license": {
                "Add necessary fields for ui to edit licenses": "https://www.drupal.org/files/issues/2018-12-02/commerce_license-allow_date_modify-3017085-6.patch"
            },
            "drupal/commerce_stock": {
                "Event subscriber assumes $order->original will always exist": "https://www.drupal.org/files/issues/2019-01-13/commerce_stock-handle_missing_original-3025662-2.patch"
            },
            "drupal/core": {
                "Selected yet disabled individual options from checkboxes element don't persist through save": "https://www.drupal.org/files/issues/2018-07-06/2911473-39.drupal.Selected-yet-disabled-individual-options-from-checkboxes-element-dont-persist-through-save.patch",
                "TypeError: Argument 1 passed to _editor_get_file_uuids_by_field() must implement interface Drupal\\\\Core\\\\Entity\\\\EntityInterface": "https://www.drupal.org/files/issues/2019-05-07/drupal-disallow-resaving-entity-when-already-being-saved-2974156-16.patch"
            },
            "drupal/ds": {
                "Add token support to css classes": "https://www.drupal.org/files/issues/2019-05-28/ds-tokens_for_layout_classes-2863420-8.patch"
            },
            "drupal/tamper": {
                "D7 EFQ Finder to D8 Entity finder" : "https://www.drupal.org/files/issues/2020-06-17/tamper-add_d7_efq_finder_to_d8_entity_finder-2976175-29.patch"
            },
            "drupal/video_embed_field": {
                "Mapping for Feeds module missing": "https://www.drupal.org/files/issues/2019-09-02/3056385-feeds-mapping-missing-5.patch"
            },
            "drupal/entity_browser": {
                "https://www.drupal.org/project/entity_browser/issues/2930677": "https://www.drupal.org/files/issues/2019-05-05/entity-browser-widget-entity-validation-2930677-55.patch"
            }
        }
    }
}
