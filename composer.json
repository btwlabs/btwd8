{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package":
            {
                "name": "sachinchoolur/lightgallery",
                "type": "drupal-library",
                "version": "1.6.11",
                "dist":
                {
                    "url": "https://github.com/sachinchoolur/lightGallery/archive/1.6.11.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/addtoany": "^1.10",
        "drupal/admin_toolbar": "^1.25",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/block_field": "^1.0@alpha",
        "drupal/block_type_templates": "^1.0@alpha",
        "drupal/commerce": "^2.13",
        "drupal/commerce_avatax": "^1.0@beta",
        "drupal/commerce_fedex": "^1.0@alpha",
        "drupal/commerce_feeds": "^1.0@alpha",
        "drupal/commerce_license": "^2.0@alpha",
        "drupal/commerce_order_number": "^1.0@alpha",
        "drupal/commerce_paypal": "^1.0@beta",
        "drupal/commerce_product_bundle": "1.x-dev",
        "drupal/commerce_reports": "^1.0@beta",
        "drupal/commerce_shipping": "^2.0@beta",
        "drupal/commerce_square": "^1.2",
        "drupal/commerce_stock": "^1.0@alpha",
        "drupal/commerce_stripe": "^1.0@beta",
        "drupal/commerce_ups": "3.x-dev",
        "drupal/commerce_usps": "^1.0",
        "drupal/commerce_variation_cart_form": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/contact_block": "^1.4",
        "drupal/contact_storage": "^1.0@beta",
        "drupal/context": "^4.0@beta",
        "drupal/core": "^8.7.0",
        "drupal/ctools": "^3.0",
        "drupal/devel": "^2.0",
        "drupal/draggableviews": "^1.2",
        "drupal/easy_breadcrumb": "^1.8",
        "drupal/email_registration": "^1.0@rc",
        "drupal/entity_pager": "^1.0@beta",
        "drupal/entity_reference_revisions": "^1.5",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/facets": "^1.1",
        "drupal/features": "^3.8",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_tamper": "2.x-dev",
        "drupal/field_group": "^1.0",
        "drupal/focal_point": "^1.0@beta",
        "drupal/fullcalendar_view": "^2.2",
        "drupal/ga_commerce": "^1.0@alpha",
        "drupal/google_analytics": "^2.3",
        "drupal/hook_event_dispatcher": "^1.20",
        "drupal/image_effects": "^2.2",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/libraries": "^3.0@alpha",
        "drupal/lightgallery": "~1.0",
        "drupal/link_attributes": "^1.5",
        "drupal/login_onlyemail": "^1.0",
        "drupal/magnific_popup": "^1.4",
        "drupal/mail_login": "^1.0",
        "drupal/mailsystem": "^4.1",
        "drupal/mandrill": "^1.1",
        "drupal/masquerade": "^2.0@beta",
        "drupal/menu_block": "^1.5",
        "drupal/menu_item_role_access": "^1.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_per_role": "^1.0@beta",
        "drupal/menu_token": "^1.0@alpha",
        "drupal/metatag": "^1.7",
        "drupal/node_view_permissions": "^1.2",
        "drupal/paragraphs": "^1.5",
        "drupal/pathauto": "^1.3",
        "drupal/physical": "^1.0@RC",
        "drupal/profile": "^1.0@RC",
        "drupal/redirect": "^1.3",
        "drupal/responsive_bg_image_formatter": "^1.2",
        "drupal/restui": "^1.16",
        "drupal/search_api": "1.11",
        "drupal/sendgrid_integration": "^1.2",
        "drupal/simple_sitemap": "^3.1",
        "drupal/slick": "^1.0",
        "drupal/slick_extras": "^1.0@beta",
        "drupal/slick_views": "^1.0@RC",
        "drupal/social_media_links": "^2.6",
        "drupal/token": "^1.5",
        "drupal/twig_tweak": "^2.1",
        "drupal/video_embed_facebook": "^1.3",
        "drupal/video_embed_field": "^2.0",
        "drupal/views_data_export": "^1.0@beta",
        "drush/drush": "^9.0.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0",
        "cweagans/composer-patches": "^1.6"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "core": ["type:drupal-core"],
            "libraries/{$name}": ["type:drupal-library"],
            "modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/contrib/{$name}": ["type:drupal-profile"],
            "themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
